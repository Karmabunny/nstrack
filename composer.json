{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "karmabunny/nstrack",
    "description": "Track namespace use in PHP",
    "type": "library",
    "keywords": ["namespace"],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Karmabunny",
            "email": "info@karmabunny.com.au"
        }
    ],
    "scripts": {
        "test": "phpunit --configuration phpunit.xml.dist",
        "analyse": "phpstan analyse -c phpstan.neon"
    },
    "bin": [
        "bin/nstrack"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^2.1"
    }
}
